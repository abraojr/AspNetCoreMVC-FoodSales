@{
    ViewData["Title"] = "UploadaFiles";
}
<img src="~/images/upload2.png" width="200" height="150" />
<hr />
<div class="jumbotron jumbotron-fluid">
    <div class="container">
        @if (ViewData["Error"] != null)
        {
            <img src="~/images/erro1.png" width="150" height="150" />
            <h4>@ViewData["Error"]</h4>
        }
    </div>
    <div class="container">
        @if (ViewData["Result"] != null)
        {
            <h4>@ViewData["Result"]</h4>
            <hr />
            @if (ViewBag.Files != null)
            {
                @foreach (string file in ViewBag.Files)
                {
                    <li>
                        @file
                    </li>
                }
            }
        }
    </div>
</div>
<div class="form-group">
    <a class="btn btn-outline-info" asp-controller="AdminImages" asp-action="GetImagens">Show Images</a>
</div>